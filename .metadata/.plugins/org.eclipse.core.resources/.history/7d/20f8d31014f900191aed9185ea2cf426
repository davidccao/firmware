/* USER CODE BEGIN (0) */
/* USER CODE END */

/* Include Files */

#include "sys_common.h"

/* USER CODE BEGIN (1) */
#include "MSP48FV_DAC_SPI.h"
/* USER CODE END */

/** @fn void main(void)
*   @brief Application main function
*   @note This function is empty by default.
*
*   This function is called after startup.
*   The user can use this function to implement the application.
*/

/* USER CODE BEGIN (2) */

uint16_t lookUpSine[]={
                       545,681,808,954,1063,1163,1253,1299,1262,1181,1072,1008,1163,1308,1435,1571,1689,1862,1944,1916,1717,1571,1390,1698,1698,1925,2125,2307,2588,2815,3033,3288,3424,3560,3560,3542,3433,3279,3024,2879,2670,2398,2189,2007,1889,1717,1880,1971,1989,1825,1671,1489,1353,1462,1617,1735,1825,1816,1644,1508,1262,1072,881
};




/* USER CODE END */

int main(void)
{
/* USER CODE BEGIN (3) */
    uint16_t i=0;
    uint32_t j=0;

  mibspiInit();

    for(i=0; i<sizeof(lookUpSine)-1; i++){
       for (  j=0;j<0xFFF; j++){
           asm("NOP");
       }
    //   MSP48FV_Set_Value(i);
//
       uint8_t try1= lookUpSine[i]/0x100;
       uint8_t try2= lookUpSine[i]%0x100;

       uint16_t txbuffer[]={0x00,try1, try2}; //debug
       mibspiSetData(mibspiREG1,0,txbuffer);
        mibspiTransfer(mibspiREG1,0);
       while(!(mibspiIsTransferComplete(mibspiREG1,0)));
    }




/* USER CODE END */

    return 0;
}


/* USER CODE BEGIN (4) */
/* USER CODE END */
